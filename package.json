{
  "name": "caticketapi",
  "version": "1.0.0",
  "description": "API del sistema CATicketJS",
  "main": "app.js",
  "scripts": {
    "prod": "tsc && npm run doc",
    "compile": "tsc && node ./dist/app.js --enable-source-maps",
    "dev": "nodemon -e ts --ignore *.json --exec \"npm run compile\"",
    "test": "jest -i --forceExit --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest -i --forceExit --detectOpenHandles --watchAll --verbose",
    "doc": "apidoc -c ./apidoc.json -i ./src/ -o ./public/docs/"
  },
  "author": "Agustin \"Cundalf\" Cundari",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-rate-limit": "^5.5.1",
    "express-validator": "^6.12.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.5.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "underscore": "^1.13.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.10",
    "@types/source-map-support": "^0.5.4",
    "@types/underscore": "^1.11.4",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  }
}
